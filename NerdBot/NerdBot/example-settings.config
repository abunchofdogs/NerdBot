<?xml version="1.0" encoding="utf-8" ?>
<NerdBot.Properties.Settings>
  <!-- The bot's ID as specified by GroupMe -->
  <setting name="BotId" serializeAs="String">
    <value>BOT_ID</value>
  </setting>
  <!-- The name of the bot in the GroupMe bot configuration. This is to prevent the bot from responding to itself. -->
  <setting name="BotName" serializeAs="String">
    <value>BOT_NAME</value>
  </setting>
  <!-- The GroupMe bots end point url. -->
  <setting name="EndPointUrl" serializeAs="String">
    <value>https://api.groupme.com/v3/bots/post</value>
  </setting>
  <!-- Secret 'route key' that the GroupMe bot is configure to post to. This is to try and prevent unauthorized posting to the bot
  e.g. http://localhost:6000/MYSECRETKEY -->
  <setting name="RouteKey" serializeAs="String">
    <value>TOKEN</value>
  </setting>
  <setting name="RouteKey" serializeAs="String">
    <value>TOKEN</value>
  </setting>
  <setting name="ConnectionString" serializeAs="String">
    <value>mongodb://localhost</value>
  </setting>
  <setting name="ConnectionDb" serializeAs="String">
    <value>mtgdb</value>
  </setting>
  <setting name="UrlShort_User" serializeAs="String">
    <value>USER</value>
  </setting>
  <setting name="UrlShort_Key" serializeAs="String">
    <value>KEY</value>
  </setting>
</NerdBot.Properties.Settings>
